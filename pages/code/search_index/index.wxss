.search_history {
  height: auto;
  width: 95%;
  position: relative;
  background-color: #fff;
  border-radius: 5rpx;
  font-size: 30rpx;
  color: #000;
  text-align: left;
}

.history_title {
  color: #999;
  text-align: left;
  padding: 10rpx;
}

.history_item {
  display: inline-block;
  padding: 10rpx 15rpx;
  background-color: rgb(248, 246, 246);
  margin: 10rpx;
  border-radius: 10rpx;
}

.clear {
  display: inline-block;
  padding: 10rpx 15rpx;
  background-color: #66d7fa;
  color: #fff;
  margin: 10rpx;
  border-radius: 10rpx;
}
